{
  "extends": ["@josselinbuils/tslint", "tslint-react"],
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "quotemark": [true, "single", "avoid-template", "jsx-double"]
  }
}
